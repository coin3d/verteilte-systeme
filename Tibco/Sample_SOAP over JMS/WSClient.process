<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.example.com/xsd/books" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:pfx="http://www.example.com/interface/books/GetBooksImpl">
    <wsdl:import namespace="http://www.example.com/interface/books/GetBooksImpl" location="/BooksService.wsdl"/>
    <pd:name>Sample_SOAP mit JMS/WSClient.process</pd:name>
    <pd:startName>Timer</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="Timer">
        <pd:type>com.tibco.plugin.timer.TimerEventSource</pd:type>
        <pd:resourceType>ae.activities.timer</pd:resourceType>
        <pd:x>54</pd:x>
        <pd:y>81</pd:y>
        <config>
            <StartTime>1102482521000</StartTime>
            <Frequency>true</Frequency>
            <TimeInterval>1</TimeInterval>
            <FrequencyIndex>Second</FrequencyIndex>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>450</pd:endX>
    <pd:endY>64</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1384431462262</pd:targetNamespace>
    <pd:activity name="SOAPRequestReply">
        <pd:type>com.tibco.plugin.soap.SOAPSendReceiveActivity</pd:type>
        <pd:resourceType>ae.activities.SOAPSendReceiveUI</pd:resourceType>
        <pd:x>229</pd:x>
        <pd:y>143</pd:y>
        <config>
            <timeout>0</timeout>
            <service>pfx:QueryBooksByAuthor</service>
            <servicePort>QueryBooksByAuthorJMSPort</servicePort>
            <operation>GetBooks</operation>
            <soapAction>/QueryBooksByAuthor</soapAction>
            <endpointURL>http://localhost:9696/QueryBooksByISBN</endpointURL>
            <sharedChannels>
                <jmsChannel>
                    <NamingURL>tibjmsnaming://localhost:7222</NamingURL>
                    <NamingInitialContextFactory>com.tibco.tibjms.naming.TibjmsInitialContextFactory</NamingInitialContextFactory>
                    <NamingPrincipal>admin</NamingPrincipal>
                    <NamingCredential/>
                    <ConnectionFactory>QueueConnectionFactory</ConnectionFactory>
                    <JMSTo>queue.sample</JMSTo>
                    <JMSMessageType>Bytes</JMSMessageType>
                    <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                    <JMSPriority>4</JMSPriority>
                    <JMSTimeToLive>0</JMSTimeToLive>
                    <JMSUserName>admin</JMSUserName>
                    <JMSPassword/>
                </jmsChannel>
            </sharedChannels>
            <soapAttachmentStyle>SwA</soapAttachmentStyle>
            <timeoutType>Seconds</timeoutType>
        </config>
        <pd:inputBindings>
            <inputMessage>
                <ns:Author>
                    <xsl:value-of select="&quot;Vivek Ranadive&quot;"/>
                </ns:Author>
            </inputMessage>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Timer</pd:from>
        <pd:to>SOAPRequestReply</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SOAPRequestReply</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>